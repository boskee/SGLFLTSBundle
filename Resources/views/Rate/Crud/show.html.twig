{% extends 'SGLFLTSBundle:Rate:layout.html.twig' %}

{% block content_title %}<h1>{{ entity.name }}</h1>{% endblock %}

{% block content %}
    {{ parent() }}

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Rate</th>
                <td>{{ entity.rate }}</td>
            </tr>
            <tr>
                <th>{% trans %}Created at{% endtrans%}</th>
                <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>{% trans %}Updated at{% endtrans%}</th>
                <td>{{ entity.updatedat|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

    {% include 'SGLFLTSBundle:Theme:Common/record_actions.html.twig' with { 'entity_name': 'rate','entity': entity } %}

{% endblock %}